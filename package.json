{
  "name": "anz-checkout-lib",
  "version": "0.1.0",
  "description": "Reusable checkout web components built with Lit.",
  "type": "module",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./anz-button": {
      "types": "./dist/anz-button.d.ts",
      "import": "./dist/anz-button.js",
      "require": "./dist/anz-button.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "npm run clean && npm run typecheck && vite build && npm run build:types",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  }
}
